{
  "name": "codexten/yii-app-site",
  "description": "Eii storage",
  "type": "library",
  "require": {
    "codexten/yii-web": "~2.0",
    "codexten/app-site-dev": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\site\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "site": [
        "$web",
        "src/config/site.php"
      ]
    }
  }
}
